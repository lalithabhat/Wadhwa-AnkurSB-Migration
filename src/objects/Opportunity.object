    <validationRules>
        <fullName>ModificationRemarksReq</fullName>
        <active>true</active>
        <description>If modification is yes modification remarks is required</description>
        <errorConditionFormula>AND(
ISPICKVAL( Modification__c  ,&quot;YES&quot;),
ISBLANK( Modification_Remarks__c ))</errorConditionFormula>
        <errorDisplayField>Modification_Remarks__c</errorDisplayField>
        <errorMessage>This field is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReasonForSiteSorceChange</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
TEXT( CCU_Source__c ) &lt;&gt; TEXT(  Site_Visit_Source__c ),
ISBLANK( Reason_for_Change_of_Source__c))</errorConditionFormula>
        <errorDisplayField>Reason_for_Change_of_Source__c</errorDisplayField>
        <errorMessage>Give Reason for change of site visit source</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReasonsforBookedReq</fullName>
        <active>true</active>
        <description>If opportunity is booked  reasons for booked is  required</description>
        <errorConditionFormula>AND(
ISPICKVAL( StageName  ,&quot;Booked&quot;),
ISPICKVAL( Reason_for_Booked_Won__c ,&quot;&quot; ))</errorConditionFormula>
        <errorDisplayField>Reason_for_Booked_Won__c</errorDisplayField>
        <errorMessage>This field is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReasonsforCancellationReq</fullName>
        <active>true</active>
        <description>If opportunity is cancelled  reasons for cancel is  required</description>
        <errorConditionFormula>AND(
ISPICKVAL( StageName  ,&quot;Cancelled&quot;),
ISPICKVAL( Reason_for_Cancellation__c ,&quot;&quot; ))</errorConditionFormula>
        <errorDisplayField>Reason_for_Cancellation__c</errorDisplayField>
        <errorMessage>This field is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ReasonsforclosedReq</fullName>
        <active>true</active>
        <description>If opportunity is closed  reasons for closed is  required</description>
        <errorConditionFormula>AND(
ISPICKVAL( StageName,&quot;Closed Lost&quot;),
ISPICKVAL( Reasons_for_closed_Lost__c,&quot;&quot; ))</errorConditionFormula>
        <errorDisplayField>Reasons_for_closed_Lost__c</errorDisplayField>
        <errorMessage>This field is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>prefereddetailCheckOpp</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Preferred_Location__c ,&quot;Others&quot;), ISBLANK( Preferred_Location_Details__c ))</errorConditionFormula>
        <errorDisplayField>Preferred_Location_Details__c</errorDisplayField>
        <errorMessage>This field is required</errorMessage>
    </validationRules>
