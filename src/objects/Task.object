    <webLinks>
        <fullName>APS_openSMS__Initiate_Chat</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Initiate Chat</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/21.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/21.0/apex.js&quot;)} 
//var allSmsCount = sforce.connection.query(&quot;Select SentSMS__c from SMSReport__c&quot;); 
//var smsCount = allSmsCount[&apos;records&apos;][&apos;SentSMS__c&apos;]; 
var smsCount= 
sforce.apex.execute(&quot;APS_openSMS.SendSMSws&quot;,&quot;UpdateAlert&quot;,{countsms:1});
if(smsCount &gt;= 100){
alert(&apos;You have reached the limit. Please upgrade your package to continue.&apos;);
}else{
var queryResult = sforce.connection.query(&quot;Select Id,APS_openSMS__SMS_Chat__c From APS_openSMS__SMS_Gateway__C where APS_openSMS__Default__c =true &quot;);
var IsSMSChatEnabled=queryResult[&apos;records&apos;][&apos;APS_openSMS__SMS_Chat__c&apos;];
if(IsSMSChatEnabled==&apos;true&apos;){
if({!Task.IsReminderSet}==true){
var name = window.location.hostname; 
var str=&apos;/apex/APS_openSMS__SMSChat?id={!Task.Who}&amp;TskID={!Task.Id}&apos;;

ChatWindow(&quot;https://&quot;+name+str, &quot;SMS Chat&quot;,300,400);

function ChatWindow(pageURL, title,w,h) {
var left = (screen.width/2)-(w/2);
var top = (screen.height/2)-(h/2);
var targetWin = window.open (pageURL, &apos;title&apos;, &apos;toolbar=no, location=yes, directories=no, status=no, menubar=no, scrollbars=no, resizable=no, copyhistory=no, width=&apos;+w+&apos;, height=&apos;+h+&apos;, top=&apos;+top+&apos;, left=&apos;+left);
