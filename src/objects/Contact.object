	var IsSMSChatEnabled = queryResult[&apos;records&apos;][&apos;APS_openSMS__SMS_Chat__c&apos;]; 
	var ChatUser = queryResult[&apos;records&apos;][&apos;APS_openSMS__ChatUsers__c&apos;]; 
	if(IsSMSChatEnabled == &apos;true&apos;){ 
		var userId = &apos;{!$User.Id}&apos;; 
		var IsChatUser = (ChatUser!=&apos;null&apos; &amp;&amp; ChatUser!=undefined &amp;&amp; ChatUser.indexOf(userId)&gt;=0); 
		if(IsChatUser){
                        var taskId = sforce.apex.execute(&quot;APS_openSMS.SendSMSws&quot;,&quot;CreateActivity&quot;,{recordId:&apos;{!Contact.Id}&apos;,nameField:&apos;name&apos;,phoneField:&apos;mobilephone&apos;});
